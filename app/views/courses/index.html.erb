<div class="container">
  <h1>Course's table </h1>
  <table class="table">
      <thead>
        <tr>
          <th>Course Name</th>
          <th>Instructor</th>
          <th>Registered seat</th>

        </tr>
      </thead>
      <tbody>
        <% @courses.each do |course| %>
        <tr>
          <td><%= link_to course.coursename, course %></td>
          <td><%= course.instructor%></td>
          <td><%= course.students.count%></td>
        </tr>
        <% end %>
      </tbody>
    </table>
</div>
